CFLAGS_REMOVE_tinympc_cf_adapter.o += -Wno-stringop-overread
CFLAGS_REMOVE_tiny_api.o += -Wno-stringop-overread
CFLAGS_REMOVE_admm.o += -Wno-stringop-overread
CFLAGS_REMOVE_rho_benchmark.o += -Wno-stringop-overread
CFLAGS_REMOVE_tiny_data.o += -Wno-stringop-overread

obj-y += controller_tinympc.o
